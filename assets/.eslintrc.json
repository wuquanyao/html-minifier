{
  "extends": "../.eslintrc.json",
  "env": {
    "browser": true,
    "es6": false,
    "worker": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "strict": [
      "error",
      "function"
    ],
    "no-var": "off",
    "object-shorthand": "off",
    "prefer-arrow-callback": "off",
    "prefer-template": "off"
  }
}
